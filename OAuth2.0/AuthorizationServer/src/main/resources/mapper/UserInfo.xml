<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oauth.modules.dao.UserInfoDao">

<select id="queryUserInfo" parameterType="java.util.Map" resultType="com.oauth.modules.entiy.UserInfoPO">
    select * from userinfo where userName = #{userName} and pwd = #{pwd}
</select>
</mapper>